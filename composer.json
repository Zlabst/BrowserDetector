{
    "name": "mimmi20/browser-detector",
    "description": "Library for PHP 5.3 to detect Browsers and Devices",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": ["Browser", "user agent", "http", "parser", "user-agent"],
    "homepage": "https://github.com/mimmi20/BrowserDetector",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/mimmi20/phpbrowscap.git"
        },
        {
            "type": "git",
            "url": "https://github.com/mimmi20/UASparser.git"
        },
        {
            "type": "git",
            "url": "https://github.com/mimmi20/kairos-uaparser.git"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "mimmi20/wurflcache": "1.0.*",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "ext-xdebug": "*",
        "synchro/uasparser": "dev-master",
        "dubcanada/uaparser": "dev-master",
        "mimmi20/wurfl": "dev-master",
        "garetjax/phpbrowscap": "dev-parser-cache",
        "monolog/monolog": "1.*@dev"
    },
    "suggest": {
        "mimmi20/wurfl": "for using a wurfl xml file as data source",
        "synchro/uasparser": "for using the uas parser as alternative parser",
        "dubcanada/uaparser": "for using the ua parser as alternative parser",
        "garetjax/phpbrowscap": "for using the browscap ini parser as alternative parser"
    },
    "autoload": {
        "psr-0": {
            "BrowserDetector\\": "library/"
        }
    }
}
