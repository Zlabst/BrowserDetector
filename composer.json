{
    "name": "mimmi20/browser-detector",
    "description": "Library for PHP 5.3 to detect Browsers and Devices",
    "type": "library",
    "license": "MIT",
    "keywords": ["Browser", "user agent", "http", "parser", "user-agent"],
    "homepage": "https://github.com/mimmi20/BrowserDetector",
    "authors": [
        {
            "name": "Thomas MÃ¼ller",
            "homepage": "https://github.com/mimmi20",
            "role": "Developer"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/mimmi20/BrowserDetector/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/mimmi20/BrowserDetector/issues",
        "source": "https://github.com/mimmi20/BrowserDetector"
    },
    "require": {
        "php": ">=5.4.0",
        "mimmi20/wurflcache": "1.2.*",
        "psr/log": "~1.0",
        "browscap/browscap": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "synchro/uasparser": "dev-master",
        "tobie/ua-parser": "dev-master",
        "mimmi20/wurfl": "dev-master",
        "browscap/browscap-php": "dev-master",
        "monolog/monolog": "1.*@dev",
        "mikey179/vfsStream": "1.3.*@dev"
    },
    "suggest": {
        "mimmi20/wurfl": "for using a wurfl xml file as data source",
        "synchro/uasparser": "for using the uas parser as alternative parser",
        "tobie/ua-parser": "for using the ua parser as alternative parser",
        "browscap/browscap-php": "for using the browscap ini parser as alternative parser",
        "crossjoin/browscap": "for using an alterantive browscap ini parser as alternative parser"
    },
    "autoload": {
        "psr-4": {
            "BrowserDetector\\": "src/",
            "BrowserDetectorTest\\": "tests/"
        }
    }
}
